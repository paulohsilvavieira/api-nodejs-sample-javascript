{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "sort-imports": "off",
    "import/order": "off",
    "no-console":"off",
    "no-undef":"off",
    "arrow-parens":"off"
  }
}
